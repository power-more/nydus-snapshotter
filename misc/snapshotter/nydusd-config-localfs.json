{
    "device": {
      "backend": {
        "type": "localfs",
        "config": {
            "dir": "/tmp/blobs/"
        }
      },
      "cache": {
        "type": "blobcache",
        "config": {
          "work_dir": "/var/lib/nydus/cache"
        }
      }
    },
    "mode": "direct",
    "digest_validate": false,
    "iostats_files": false,
    "enable_xattr": true,
    "fs_prefetch": {
      "enable": true,
      "threads_count": 2
    }
  }
  